<Page x:Class="MMK.SmartSystem.Laser.Base.MachineOperation.ContainerPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MMK.SmartSystem.Laser.Base.MachineOperation"
        mc:Ignorable="d"
        xmlns:embed="clr-namespace:MMK.SmartSystem.Common.Embed;assembly=MMK.SmartSystem.Common"
        d:DesignHeight="658" d:DesignWidth="1268" Background="#3b3b3b" FontSize="18" FontFamily="思源黑体 CN Medium" Foreground="#FFB6BEC3">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <StackPanel VerticalAlignment="Center">
            <Button Content="弹框" x:Name="btnGo" Click="btnGo_Click" Width="100" Height="50"/>

        </StackPanel>
        <DockPanel Grid.Column="1" LastChildFill="True">
            <embed:AppContainer x:Name="appEmbed" Width="800" Height="500"/>
        </DockPanel>
    </Grid>
</Page>
