<Page x:Class="MMK.SmartSystem.Laser.Base.MachineMonitor.MachineMonitorPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MMK.SmartSystem.Laser.Base.MachineMonitor"
      mc:Ignorable="d" 
      d:DesignHeight="500" d:DesignWidth="1200"
      Title="MachineMonitorPage">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <local:CoordinateControl Width="600" Height="500" Grid.Column="0" x:Name="coordinateControl"/>
        <local:ProgramPathControl Width="600" Height="500" Grid.Column="1" x:Name="programPathControl"/>
    </Grid>
</Page>
